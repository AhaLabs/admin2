---
import Layout from "../../layouts/Layout.astro";
import { getContractData } from "../../protocols/near";
import { ClientSideEntryPoint } from "../../react";

const { contract } = Astro.params;
const data = await getContractData(contract);

if (!contract || !data) return Astro.redirect("/404");
---

<Layout contractData={data}>
  <ClientSideEntryPoint client:only="react" />
</Layout>
