---
import Layout from "../../../layouts/Layout.astro";
import { getData } from "../../../protocols/near";
import { ClientSideEntryPoint } from "../../../react";

const { contract, method } = Astro.params;
const data = await getData(contract, method);

if (!contract || !data) return Astro.redirect("/404");
---

<Layout contractData={data}>
  <ClientSideEntryPoint client:only />
</Layout>
